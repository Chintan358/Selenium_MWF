package statements;

public class S_IF_ELSe {
	public static void main(String[] args) {
		
		
		int a =45;
		int b = 300;
		int c = 400;
		
		if(a>b)
		{
			if(a>c) {
				System.out.println("a is greater");
			}
			else
			{
				System.out.println("c is greater");
			}
			
		}
		else
		{
			if(b>c) {
			System.out.println("b is greater");
			}
			else
			{
				System.out.println("c is greater");
			}
		}
		
		
	}
}
