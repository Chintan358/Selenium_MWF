package oops;

class Emp
{
	int id;
	String name;
	double sal;
	
	Emp()
	{
		System.out.println("Constructor calling...");
	}
	
	Emp(String name)
	{
		System.out.println("Name is : "+name);
	}
	
	Emp(int id)
	{
		System.out.println("id is : "+id);
	}
	public void display()
	{
		System.out.println(id+" "+name+" "+sal);
	}
}

public class O003_constructorDemo {
	public static void main(String[] args) {
		
		Emp e = new Emp();
		
		Emp e1 = new Emp("Tops");
		
		Emp e2 = new Emp(10);
	}
}
