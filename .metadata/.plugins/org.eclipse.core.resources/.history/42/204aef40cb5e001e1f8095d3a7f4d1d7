package oops;

class A
{
	int id = 10;
	String name = "test";
	
	public void display()
	{
		System.out.println("Runing class a display");
		System.out.println(id+" "+name);
	}
}



class C extends B
{
	
}
public class O007_InheritanceDemo {
	public static void main(String[] args) {
		
		B b  =new B();
		b.show();
	}
}
